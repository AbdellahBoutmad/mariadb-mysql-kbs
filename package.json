{
    "name": "mariadb-mysql-kbs",
    "version": "1.2.4",
    "description": "An index of the MariaDB and MySQL Knowledge bases",
    "main": "src/index.js",
    "homepage": "https://github.com/williamdes/mariadb-mysql-kbs#readme",
    "readme": "https://github.com/williamdes/mariadb-mysql-kbs/blob/master/README.md",
    "license": "MPL-2.0",
    "bugs": {
        "url": "https://github.com/williamdes/mariadb-mysql-kbs/issues",
        "email": "williamdes@wdes.fr"
    },
    "keywords": [
        "mariadb",
        "mysql",
        "dataset",
        "kb",
        "knowledge-base",
        "json",
        "library",
        "mysql-knowledge-bases",
        "mariadb-knowledge-bases",
        "composer-package",
        "npm-package"
    ],
    "nyc": {
        "exclude": [
            "apidocs",
            "data",
            "test",
            "build",
            "vendor",
            "coverage",
            ".nyc_output"
        ],
        "reporter": [
            "text",
            "text-summary"
        ]
    },
    "scripts": {
        "prettier": "prettier \"*/**/*.js\" --ignore-path ./.prettierignore --write",
        "test": "nyc mocha --ui tdd",
        "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov",
        "sudo-bot-pr": "sudo-bot --env scripts/sudo-bot/.env --verbose"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/williamdes/mariadb-mysql-kbs.git"
    },
    "author": {
        "email": "williamdes@wdes.fr",
        "name": "William Desportes",
        "url": "https://william.wdes.fr"
    },
    "dependencies": {
        "jsdom": "^13.1.0"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "codecov": "^3.1.0",
        "mocha": "^5.2.0",
        "nyc": "^13.1.0",
        "prettier": "^1.15.3",
        "sudo-bot": "^1.1.4",
        "twig": "^1.13.0"
    },
    "directories": {
        "lib": "src",
        "test": "test"
    },
    "prettier": {
        "singleQuote": true,
        "proseWrap": "never",
        "trailingComma": "es5",
        "printWidth": 95
    },
    "publishConfig": {
        "access": "public"
    }
}
